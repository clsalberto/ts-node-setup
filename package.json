{
  "name": "ts-node-setup",
  "version": "1.0.0",
  "main": "src/main/server.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "dev:queue": "tsnd -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/queue.ts",
    "test": "jest",
    "start": "node dist/server.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.15.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.14",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prisma": "^3.9.1",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "3.9.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.5.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "mongoose": "^6.2.0",
    "nodemailer": "^6.7.2",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
